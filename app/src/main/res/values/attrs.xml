<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RTCGlView">
        <attr name="mainMirror" format="boolean" />
        <attr name="secondMirror" format="boolean" />
        <attr name="mainCoords" format="reference" />
        <attr name="secondCoords" format="reference" />
    </declare-styleable>

    <declare-styleable name="SeekBarPreference">
        <attr name="min" format="integer" />
        <attr name="stepSize" format="integer" />
    </declare-styleable>

    <declare-styleable name="MyCustomSwitch">
        <attr name="leftSwitchText" format="string|reference" />
        <attr name="rightSwitchText" format="string|reference" />
    </declare-styleable>

    <attr name="backgroundTheme" format="reference" />
    <attr name="backgroundChat" format="color" />

    <declare-styleable name="MaxHeightRecyclerView">
        <attr name="maxViewHeight" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="MaxHeightScrollView">
        <attr name="maxScrollViewHeight" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="SettingItem">
        <attr name="showDivider" format="boolean" />
        <attr name="leftIcon" format="reference" />
        <attr name="leftIconTint" format="reference" />
        <attr name="titleText" format="string|reference" />
        <attr name="titleColor" format="reference" />
    </declare-styleable>
</resources>